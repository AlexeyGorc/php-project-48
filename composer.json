{
    "name": "hexlet/code",
    "license": "proprietary",
    "description": "compare json or yaml files",
    "type": "project",
    "bin": [
        "/bin/gendiff"
    ],
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src/"
        },
        "files" : [
            "src/Differ.php",
            "src/Parsers.php",
            "src/Formatters.php",
            "src/Formatters/Json.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Plain.php"
        ]
    },
    "authors": [
        {
            "name": "AlexeyGoncharenko",
            "email": "zomorc@gmail.com"
        }
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "lstrojny/functional-php": "^1.17",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "squizlabs/php_codesniffer": "^3.8"
    }
}
